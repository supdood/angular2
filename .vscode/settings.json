// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).ts"},
        "**/*.js.map": true,
        "package.json": true,
        "systemjs.config.js": true,
        "typings.json": true,
        "tsconfig.json": true,
        "node_modules": true,
        "typings": true,
        ".vscode": true
    }
}